{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "ROS: catkin_make",
            "type": "shell",
            "command": "catkin_make",
            "args": [
                "-DCMAKE_BUILD_TYPE=RelWithDebInfo",
                "-DCATKIN_BLACKLIST_PACKAGES=node_tester"
            ],
            "problemMatcher": [],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "ROS catkin_make clean",
            "type": "shell",
            "command": "rm -rf build/ devel/",
            "problemMatcher":[],
            "group": {
                "kind": "build",
                "isDefault": "none"
            }
        }
    ]
}